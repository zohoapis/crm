{
  "name": "@zohoapis/crm",
  "version": "0.1.1",
  "description": "Library to simplify calling Zoho CRM's APIs.",
  "repository": {
    "type": "git",
    "url": "https://github.com/zohoapis/crm.git"
  },
  "homepage": "https://github.com/zohoapis/crm",
  "bugs": "https://github.com/zohoapis/crm/issues",
  "author": {
    "name": "zohoapis"
  },
  "keywords": [
    "zoho",
    "api",
    "node",
    "crm"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "packageManager": "pnpm@8.5.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {}
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest watch --ui",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.2.3",
    "@vitest/ui": "^0.31.1",
    "c8": "^7.13.0",
    "minifaker": "^1.34.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
  }
}
